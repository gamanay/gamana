git init

git config --global user.name "gamana"
git config --global user.email "gamanay15@gmail.com"

git config --list

phases
workspace area
staging/index area
local repository

touch file1
ls
git status
git add file1
git commit -m "sample commit" file1
git status
git log
git show cid

adding multiple files
touch file2 file3 file4 file5
git status
git add *
or
git add .
or
git add -A
git status
git commit -m "label"(all files will be commited)

git reset HEAD file-name(staging to workspace)
git reset --soft cid(local to staging)
git reset --mixed cid(local to workspace)

git commit -m "label" file-name(workspace to local only modified files)

git clone https://github.com/gamanay/gamana.git
ls
cd gamana
touch gamana1
git status
git add gamana1
git commit -m "file adding"
git push
